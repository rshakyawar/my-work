            %li
              %a{:class => "queTwo",:href=>"javascript:{}",:onclick=>"change_question_value(2);"}
                %span Question 2
            %li
              %a{:class => "queThree",:href=>"javascript:{}",:onclick=>"change_question_value(3);"}
                %span Question 3
  %p.quesSty
    %a{:href => "/brandpartners/#{Product::BLOCK_IMAGE_NAME["versus_question"]}",}
      %img{:alt => "Your Question", :src => "/images/btn_question.png"}/
