= stylesheet_link_tag 'membership_survey'

- survey_three_obj = get_member_survey_three @member  
- survey_two_obj = get_member_survey_two @member 
.membership_content
  .membership_heading
  / communication box start  
  -if @member.errors.any?
    #error_explanation{:style=>"margin-left:145px;margin-top:50px;text-align:left;color:red;"}
      %h3= "#{pluralize(@member.errors.count, "error")} prohibited this survey from being saved:"
      %ul{:style=>"margin-left:15px"}
        - @member.errors.full_messages.each do |msg|
          %li= msg     
  = nested_form_for :member ,  :url => save_survey_path(:id=>@member.id) do |outer_form|         
    = render :partial=>"survey_one",:locals => {:inner_form => outer_form, :survey_three_obj => survey_three_obj}   
    = render :partial => 'survey_two',:locals => {:inner_form => outer_form, :survey_two_obj => survey_two_obj}
    = render :partial => 'survey_three' ,:locals => {:inner_form => outer_form, :survey_three_obj => survey_three_obj}   
.membership_next_bttn_row
  .membership_next_bttn
    %a{:href => "javascript:void(0)",:onclick=>"validate_survey_one();"}
#popup1.popupbox
  %div.survey_two_pop_up{:style=>"font-size:18px;"} Just click on what you love!
  %div.survey_two_pop_up
    #select_1_id
      - APPLICATION_CONFIG.member_survey.survey_2.books.each do |val|
        = check_box_tag 'ms_two_books', val, (!survey_two_obj.books.nil? and (survey_two_obj.books.include?(val) or survey_two_obj.books.include?(" "+val))),{:id=>"books" + val}   
        %label{:for=>"books" + val}
          = val      
        %br
